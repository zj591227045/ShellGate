{
  "name": "shellgate",
  "version": "1.0.0",
  "description": "Web-based remote control tool supporting SSH, Telnet, RDP, VNC, SFTP protocols",
  "main": "dist/server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "PORT=4001 nodemon --exec ts-node src/server/index.ts",
    "dev:client": "cd src/client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "tsc -p tsconfig.server.json",
    "build:client": "cd src/client && npm run build",
    "start": "node dist/server/index.js",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "install:client": "cd src/client && npm install"
  },
  "keywords": [
    "ssh",
    "telnet",
    "rdp",
    "vnc",
    "sftp",
    "remote",
    "terminal",
    "web"
  ],
  "author": "ShellGate Team",
  "license": "MIT",
  "dependencies": {
    "@types/sqlite3": "^5.1.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "node-ssh": "^13.1.0",
    "socket.io": "^4.8.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "telnet-client": "^2.0.3",
    "uuid": "^9.0.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.5",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.8.0",
    "@types/uuid": "^9.0.6",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
